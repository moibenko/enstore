#This cronjob creates and enstore log web page containing all the nodes
# that can mount pnfs.
12 * * * * root source /usr/local/etc/setups.sh; setup enstore; $ENSTORE_DIR/sbin/ecron -p pnfsExports $ENSTORE_DIR/sbin/chimeraExportList >/dev/null 2>/dev/null
12 5 * * * root source /usr/local/etc/setups.sh; setup enstore; $ENSTORE_DIR/sbin/ecron -p pnfs_monitor $ENSTORE_DIR/sbin/pnfs_monitor >/dev/null 2>/dev/null
